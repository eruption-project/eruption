complete -c eruption-audio-proxy -n "__fish_use_subcommand" -s v -l verbose -d 'Verbose mode (-v, -vv, -vvv, etc.)'
complete -c eruption-audio-proxy -n "__fish_use_subcommand" -f -a "daemon" -d 'Run in background'
complete -c eruption-audio-proxy -n "__fish_use_subcommand" -f -a "completions" -d 'Generate shell completions'
complete -c eruption-audio-proxy -n "__fish_use_subcommand" -f -a "help" -d 'Print this message or the help of the given subcommand(s)'
