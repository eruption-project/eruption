(( $+functions[_eruptionctl_commands] )) ||
_eruptionctl_commands() {
    local commands; commands=(
'status:Zeigt das aktuell aktive Profil oder den Slot' \
'switch:Zu einem anderen Profil im aktiven Slot, oder einem anderen Slot wechseln' \
'config:Laufzeit Konfiguration des Eruption Daemons bearbeiten' \
'devices:Gerätespezifische Konfiguration bearbeiten' \
'profiles:Profil bezogene Unterbefehle' \
'scripts:Script bezogene Unterbefehle' \
'color-schemes:Ein Farbschema definieren, importieren oder löschen' \
'param:Lesen oder schreiben von Parameterwerten von aktuell laufenden Scripten' \
'names:Slot-Namen anzeigen oder ändern' \
'effects:Spezialeffekte wie z.B. Ambient, Bildoverlays oder Animationen' \
'rules:Automatisierungsregeln anzeigen und verwalten' \
'completions:Shell Definitionen für Autovervollständigung erzeugen' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'eruptionctl commands' commands "$@"
}
