'-h[Print help]' \
'--help[Print help]' \
':profile_name:' \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" \
":: :_eruptionctl__profiles__help_commands" \
"*::: :->help" \
&& ret=0

    case $state in
    (help)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
