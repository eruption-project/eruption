&& ret=0
;;
        esac
    ;;
esac
;;
        esac
    ;;
esac
;;
(scripts)
_arguments "${_arguments_options[@]}" \
'-h[Print help]' \
'--help[Print help]' \
":: :_eruptionctl__scripts_commands" \
