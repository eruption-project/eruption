'-h[Print help]' \
'--help[Print help]' \
'::script:' \
'::parameter:' \
'::value:' \
&& ret=0
;;
(names)
_arguments "${_arguments_options[@]}" \
'-h[Print help]' \
'--help[Print help]' \
":: :_eruptionctl__names_commands" \
"*::: :->names" \
&& ret=0

